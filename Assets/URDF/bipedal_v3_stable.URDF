<?xml version="1.0"?>
<robot name="bipedal_v3">

  <material name="silver"><color rgba="0.7 0.7 0.7 1"/></material>
  <material name="red"><color rgba="1 0 0 1"/></material>
  <material name="black"><color rgba="0.1 0.1 0.1 1"/></material>
  <material name="blue"><color rgba="0 0 0.8 1"/></material>

  <link name="base_link">
    <visual>
      <origin xyz="0.06 0 0" rpy="0 0 0"/>
      <geometry><box size="0.25 0.5 0.2"/></geometry>
      <material name="blue"/>
    </visual>
    <inertial>
      <origin xyz="0.06 0 0" rpy="0 0 0"/>
      <mass value="3.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <joint name="l_hip_yaw_joint" type="revolute">
    <parent link="base_link"/>
    <child link="l_hip_yaw_link"/>
    <origin xyz="0 0.15 -0.1" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
  </joint>

  <link name="l_hip_yaw_link">
    <visual><geometry><sphere radius="0.04"/></geometry><material name="red"/></visual>
    <inertial><mass value="0.1"/><inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/></inertial>
  </link>

  <joint name="l_hip_roll_joint" type="revolute">
    <parent link="l_hip_yaw_link"/>
    <child link="l_hip_roll_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/> 
    <limit lower="-0.5" upper="0.5" effort="10" velocity="1.0"/>
  </joint>

  <link name="l_hip_roll_link">
    <visual><geometry><sphere radius="0.04"/></geometry><material name="red"/></visual>
    <inertial><mass value="0.1"/><inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/></inertial>
  </link>

  <joint name="l_hip_pitch_joint" type="revolute">
    <parent link="l_hip_roll_link"/>
    <child link="l_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/> 
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
  </joint>

  <link name="l_thigh_link">
    <visual>
      <origin xyz="0 0 -0.15"/>
      <geometry><cylinder radius="0.025" length="0.3"/></geometry>
      <material name="silver"/>
    </visual>
    <inertial>
      <origin xyz="0 0 -0.15"/><mass value="1.0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <joint name="l_knee_joint" type="revolute">
    <parent link="l_thigh_link"/>
    <child link="l_shank_link"/>
    <origin xyz="0 0 -0.3"/>
    <axis xyz="0 -1 0"/>
    <limit lower="-2.0" upper="0" effort="10" velocity="1.0"/>
  </joint>

  <link name="l_shank_link">
    <visual><origin xyz="0 0 -0.15"/><geometry><cylinder radius="0.025" length="0.3"/></geometry><material name="silver"/></visual>
    <visual><origin xyz="0 0 0"/><geometry><sphere radius="0.035"/></geometry><material name="red"/></visual>
    <inertial><origin xyz="0 0 -0.15"/><mass value="1.0"/><inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.01"/></inertial>
  </link>

  <joint name="l_ankle_pitch_joint" type="revolute">
    <parent link="l_shank_link"/>
    <child link="l_ankle_pitch_link"/>
    <origin xyz="0 0 -0.3"/><axis xyz="0 -1 0"/>
    <limit lower="-0.8" upper="0.8" effort="10" velocity="1.0"/>
  </joint>

  <link name="l_ankle_pitch_link">
    <visual><geometry><sphere radius="0.03"/></geometry><material name="red"/></visual>
    <inertial><mass value="0.1"/><inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/></inertial>
  </link>

  <joint name="l_ankle_roll_joint" type="revolute">
    <parent link="l_ankle_pitch_link"/>
    <child link="l_foot_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.4" upper="0.4" effort="10" velocity="1.0"/>
  </joint>

  <link name="l_foot_link">
    <visual><origin xyz="0.05 0 -0.02"/><geometry><box size="0.25 0.12 0.04"/></geometry><material name="black"/></visual>
    <inertial><origin xyz="0.05 0 -0.02"/><mass value="1"/><inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/></inertial>
  </link>

  <joint name="r_hip_yaw_joint" type="revolute">
    <parent link="base_link"/>
    <child link="r_hip_yaw_link"/>
    <origin xyz="0 -0.15 -0.1" rpy="0 0 0"/>
    <axis xyz="0 0 -1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
  </joint>

  <link name="r_hip_yaw_link">
    <visual><geometry><sphere radius="0.04"/></geometry><material name="red"/></visual>
    <inertial><mass value="0.1"/><inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/></inertial>
  </link>

  <joint name="r_hip_roll_joint" type="revolute">
    <parent link="r_hip_yaw_link"/>
    <child link="r_hip_roll_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="-1 0 0"/> 
    <limit lower="-0.5" upper="0.5" effort="10" velocity="1.0"/>
  </joint>

  <link name="r_hip_roll_link">
    <visual><geometry><sphere radius="0.04"/></geometry><material name="red"/></visual>
    <inertial><mass value="0.1"/><inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/></inertial>
  </link>

  <joint name="r_hip_pitch_joint" type="revolute">
    <parent link="r_hip_roll_link"/>
    <child link="r_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/> 
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
  </joint>

  <link name="r_thigh_link">
    <visual><origin xyz="0 0 -0.15"/><geometry><cylinder radius="0.025" length="0.3"/></geometry><material name="silver"/></visual>
    <inertial><origin xyz="0 0 -0.15"/><mass value="1.0"/><inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.01"/></inertial>
  </link>

  <joint name="r_knee_joint" type="revolute">
    <parent link="r_thigh_link"/>
    <child link="r_shank_link"/>
    <origin xyz="0 0 -0.3"/>
    <axis xyz="0 -1 0"/>
    <limit lower="-2.0" upper="0" effort="10" velocity="1.0"/>
  </joint>

  <link name="r_shank_link">
    <visual><origin xyz="0 0 -0.15"/><geometry><cylinder radius="0.025" length="0.3"/></geometry><material name="silver"/></visual>
    <visual><origin xyz="0 0 0"/><geometry><sphere radius="0.035"/></geometry><material name="red"/></visual>
    <inertial><origin xyz="0 0 -0.15"/><mass value="1.0"/><inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.01"/></inertial>
  </link>

  <joint name="r_ankle_pitch_joint" type="revolute">
    <parent link="r_shank_link"/>
    <child link="r_ankle_pitch_link"/>
    <origin xyz="0 0 -0.3"/><axis xyz="0 -1 0"/>
    <limit lower="-0.8" upper="0.8" effort="10" velocity="1.0"/>
  </joint>

  <link name="r_ankle_pitch_link">
    <visual><geometry><sphere radius="0.03"/></geometry><material name="red"/></visual>
    <inertial><mass value="0.1"/><inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/></inertial>
  </link>

  <joint name="r_ankle_roll_joint" type="revolute">
    <parent link="r_ankle_pitch_link"/>
    <child link="r_foot_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="-1 0 0"/>
    <limit lower="-0.4" upper="0.4" effort="10" velocity="1.0"/>
  </joint>

  <link name="r_foot_link">
    <visual><origin xyz="0.05 0 -0.02"/><geometry><box size="0.25 0.12 0.04"/></geometry><material name="black"/></visual>
    <inertial><origin xyz="0.05 0 -0.02"/><mass value="1"/><inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/></inertial>
  </link>

</robot>
